{
  "name": "infection-visualizer",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "files": [
      "dist/**/*",
      "main.js",
      "package.json",
      {
        "from": "backend",
        "to": "backend",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "portable_python",
        "to": "portable_python",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "backend",
        "to": "backend"
      },
      {
        "from": "portable_python",
        "to": "portable_python"
      }
    ],
    "appId": "com.infection.visualizer",
    "productName": "Infection Visualizer",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
